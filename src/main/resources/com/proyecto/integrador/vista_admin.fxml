<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.proyecto.integrador.VistaAdminFXController"
      spacing="12" alignment="CENTER">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Administración" style="-fx-font-size:16;" />
    <Button text="Módulo Ubicación" onAction="#onModuloUbicacion" prefWidth="220"/>
    <!-- Añadir botones para Módulo Denuncia, Módulo LugarDenuncia -->
    <Button text="Regresar" onAction="#onBack" prefWidth="220"/>
</VBox>
